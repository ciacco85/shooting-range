{
  "name": "shooting-range",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "swa": "swa start http://localhost:5173 --run \"npm run dev\"",
    "devswa": "concurrently --kill-others \"npm run dev\" \"npm run swa\"",
    "production-deploy": "vite build -- && swa deploy .\\dist\\ -R TestApp -n shooting-range --env production -d TOKEN"
  },
  "dependencies": {
    "pinia": "^2.2.6",
    "roboto-fontface": "*",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5",
    "vuetify": "^3.4.8",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^2.0.1",
    "@mdi/font": "7.4.47",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@types/webfontloader": "^1.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^7.0.1",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vuetify": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-tsc": "^2.1.10"
  }
}